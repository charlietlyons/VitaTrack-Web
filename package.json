{
  "name": "vitatrack-web",
  "version": "1.0.0",
  "description": "A web interface for the VitaTrackAPI",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode production",
    "dev": "concurrently \"npm run stub-server\" \"npm run start:dev\"",
    "start:dev": "webpack serve --mode development",
    "stub-server": "kill-port 3000 && node stubserver.js",
    "kill-stub-server": "kill-port 3000",
    "test": "jest --env=jsdom",
    "test:one": "jest -t Login --watch --coverage --env=jsdom",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --watchAll --env=jsdom",
    "cypress:open": "cypress open",
    "cypress:run": "concurrently \"npm run start:dev\" \"cypress run\"",
    "coverage:open": "open coverage/lcov-report/index.html"
  },
  "author": "Charlotte Lyons",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.13.5",
    "axios": "^1.4.0",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "styled-components": "^6.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@cypress/code-coverage": "^3.12.5",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@webpack-cli/serve": "^2.0.5",
    "babel-loader": "^9.1.2",
    "babel-plugin-istanbul": "^6.1.1",
    "concurrently": "^8.2.0",
    "cypress": "^13.3.1",
    "dotenv": "^16.1.3",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.3",
    "kill-port": "^2.0.1",
    "nyc": "^15.1.0",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "jest": {
    "resetMocks": true,
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/*.spec.js"
    ],
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coveragePathIgnorePatterns": [
      "./src/index.js",
      "./src/App.jsx"
    ]
  }
}
